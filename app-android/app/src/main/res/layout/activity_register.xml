        app:layout_constraintTop_toBottomOf="@+id/edCedulaReg" />

    <EditText
        android:id="@+id/edContrasenaReg"
        android:layout_width="368dp"
        android:layout_height="48dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="2dp"
